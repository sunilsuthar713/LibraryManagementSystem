<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="javafxapplication.ui.listAuthor.AuthorListController">
    <stylesheets>
        <URL value="@author_list.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableView" prefHeight="410.0" prefWidth="610.0">
         <columns>
            <TableColumn fx:id="nameCol" prefWidth="75.0" text="Author Name" />
            <TableColumn fx:id="idCol" prefWidth="75.0" text="Book ID" />
            <TableColumn fx:id="categoryCol" prefWidth="75.0" text="Category" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
